<h1><%= @user.user_name %>'s Movie Discover Page</h1>

<%= button_to 'Discover Top Rated Movies', movies_path, method: :get  %> 

<%= form_with url: "/movies?q=keyword", method: :get, local: true do |f| %>
  <%= f.text_field :search, placeholder: 'Movie Title' %>
  <%= f.submit "Search" %>
<% end %>