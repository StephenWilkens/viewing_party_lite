<h1> Viewing Party </h1>

<% if current_user %>
  <p>You are logged in!</p>
<% else %>
  <%= button_to "Create a New User", '/register', method: :get %>
  <%= button_to 'Log In', '/login', method: :get %>
<% end %>

<h3> Existing Users </h3>
<div id=existing-users>
  <% @users.each do |user| %>
    <%= link_to "#{user.user_name}", user_path(user.id) %>
  <% end %>
</div>
